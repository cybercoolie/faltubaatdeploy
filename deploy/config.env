# FaltuBaat Deploy Configuration
# ================================
# This file configures where to download the application code from.
# Edit this file before running deployment scripts.

# GitHub Repository URL (HTTPS)
# Change this to your application code repository
GITHUB_REPO="https://github.com/YOUR_ORG/faltubaat.git"

# Branch or tag to deploy
GITHUB_BRANCH="main"

# Alternative: Use a specific release tag
# GITHUB_BRANCH="v1.0.0"

# Docker image registry (for container deployments)
# Leave empty to build locally, or set to pull from registry
DOCKER_REGISTRY=""
DOCKER_IMAGE_NAME="faltubaat"
DOCKER_IMAGE_TAG="latest"

# Example for AWS ECR:
# DOCKER_REGISTRY="123456789.dkr.ecr.us-east-1.amazonaws.com"
# DOCKER_IMAGE_NAME="faltubaat"
# DOCKER_IMAGE_TAG="latest"
